{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "jupyterhub": {
            "type": "object",
            "properties": {
                "cull": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean",
                            "form": true,
                            "title": "Enable removal of idle notebooks"
                        }
                    }
                }
            }
        },
        "ingress": {
            "type": "boolean",
            "form": true,
            "title": "Use a custom hostname",
            "description": "Enable the ingress resource that allows you to access the JHUB installation."
        },
        "hostname": {
                    "type": "string",
                    "form": true,
                    "hidden": {
                        "value": false,
                        "path": "ingress"
                    },
                    "title": "Hostname"
        },
        "spark": {
                    "type": "object",
                    "title": "Enable spark submission",
                    "properties": {
                        "enabled": {
                            "type": "boolean",
                            "form": true
                        }
                    }
        }
    }
}