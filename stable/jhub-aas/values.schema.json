{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "jupyterhub": {
            "type": "object",
            "properties": {
                "cull": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "every": {
                            "type": "integer"
                        },
                        "timeout": {
                            "type": "integer"
                        }
                    }
                },
                "hub": {
                    "type": "object",
                    "properties": {
                        "args": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        },
                        "cookieSecret": {
                            "type": "string"
                        },
                        "db": {
                            "type": "object",
                            "properties": {
                                "pvc": {
                                    "type": "object",
                                    "properties": {
                                        "accessModes": {
                                            "type": "array",
                                            "items": {
                                                "type": "string"
                                            }
                                        },
                                        "storage": {
                                            "type": "string"
                                        },
                                        "storageClassName": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "type": {
                                    "type": "string"
                                },
                                "upgrade": {
                                    "type": "null"
                                }
                            }
                        },
                        "extraConfig": {
                            "type": "object",
                            "properties": {
                                "myConfig.py": {
                                    "type": "string"
                                }
                            }
                        },
                        "extraEnv": {
                            "type": "object",
                            "properties": {
                                "ADMIN_OAUTH_GROUPS": {
                                    "type": "string"
                                },
                                "OAUTH_CALLBACK_URL": {
                                    "type": "string"
                                },
                                "OAUTH_ENDPOINT": {
                                    "type": "string"
                                },
                                "OAUTH_GROUPS": {
                                    "type": "string"
                                },
                                "WITH_GPU": {
                                    "type": "string"
                                }
                            }
                        },
                        "image": {
                            "type": "object",
                            "properties": {
                                "name": {
                                    "type": "string"
                                },
                                "tag": {
                                    "type": "string"
                                }
                            }
                        },
                        "networkPolicy": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                }
                            }
                        }
                    }
                },
                "ingress": {
                    "type": "object",
                    "properties": {
                        "annotations": {
                            "type": "object",
                            "properties": {
                                "kubernetes.io/ingress.class": {
                                    "type": "string"
                                }
                            }
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "hosts": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        },
                        "tls": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "hosts": {
                                        "type": "array",
                                        "items": {
                                            "type": "string"
                                        }
                                    },
                                    "secretName": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "proxy": {
                    "type": "object",
                    "properties": {
                        "chp": {
                            "type": "object",
                            "properties": {
                                "networkPolicy": {
                                    "type": "object",
                                    "properties": {
                                        "enabled": {
                                            "type": "boolean"
                                        }
                                    }
                                }
                            }
                        },
                        "secretToken": {
                            "type": "string"
                        }
                    }
                },
                "scheduling": {
                    "type": "object",
                    "properties": {
                        "podPriority": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                }
                            }
                        },
                        "userPlaceholder": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                }
                            }
                        },
                        "userScheduler": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                }
                            }
                        }
                    }
                },
                "singleuser": {
                    "type": "object",
                    "properties": {
                        "networkPolicy": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                }
                            }
                        },
                        "storage": {
                            "type": "object",
                            "properties": {
                                "dynamic": {
                                    "type": "object",
                                    "properties": {
                                        "storageClass": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "spark": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                }
            }
        }
    }
}