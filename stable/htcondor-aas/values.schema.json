{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "autoscaling": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "maxReplicas": {
                    "type": "integer"
                },
                "minReplicas": {
                    "type": "integer"
                },
                "targetMetric": {
                    "type": "string"
                },
                "targetMetricValue": {
                    "type": "number"
                }
            }
        },
        "cluster": {
            "type": "object",
            "properties": {
                "secret": {
                    "form": true,
                    "type": "string",
                    "title": "Cluster shared secret"
                }
            }
        },
        "cvmfs": {
            "type": "object",
            "properties": {
                "defaultLocalConfig": {
                    "type": "array"
                },
                "enabled": {
                    "type": "boolean"
                },
                "image": {
                    "type": "string"
                },
                "privKey": {
                    "type": "array"
                },
                "pullPolicy": {
                    "type": "string"
                },
                "repoList": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "htcClient": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "pullPolicy": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "master": {
            "type": "object",
            "properties": {
                "extraconfig": {
                    "type": "string"
                },
                "hostname": {
                    "form": true,
                    "type": "string"
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "pullPolicy": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "publicIP": {
                    "form": true,
                    "type": "string"
                },
                "requests": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                },
                "service": {
                    "type": "object",
                    "properties": {
                        "nodePort": {
                            "type": "integer"
                        },
                        "targetPort": {
                            "type": "integer"
                        },
                        "type": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "prometheusExporter": {
            "type": "object",
            "properties": {
                "image": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "pullPolicy": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "schedd": {
            "type": "object",
            "properties": {
                "extraconfig": {
                    "type": "string"
                },
                "hostname": {
                    "form": true,
                    "type": "string"
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "pullPolicy": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "mapfile": {
                    "type": "string"
                },
                "persistence": {
                    "type": "object",
                    "properties": {
                        "spooldir": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                },
                                "size": {
                                    "type": "string",
                                    "title": "Schedd Spool Volume Size",
                                    "form": true,
                                    "render": "slider",
                                    "sliderMin": 1,
                                    "sliderMax": 100,
                                    "sliderUnit": "Gi"
                                },
                                "storageClass": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "requests": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string",
                            "title": "Schedd CPUs",
                            "form": true,
                            "render": "slider",
                            "sliderMin": 1,
                            "sliderMax": 10000,
                            "sliderUnit": "m"
                        },
                        "memory": {
                            "type": "string",
                            "title": "Schedd Memory",
                            "form": true,
                            "render": "slider",
                            "sliderMin": 1,
                            "sliderMax": 16000,
                            "sliderUnit": "Mi"
                        }
                    }
                },
                "service": {
                    "type": "object",
                    "properties": {
                        "nodePort": {
                            "type": "integer"
                        },
                        "targetPort": {
                            "type": "integer"
                        },
                        "type": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "squid": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "image": {
                    "type": "string"
                },
                "port": {
                    "type": "integer"
                },
                "pullPolicy": {
                    "type": "string"
                },
                "replicas": {
                    "type": "integer"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "wn": {
            "type": "object",
            "properties": {
                "image": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "form": true,
                            "type": "string"
                        },
                        "pullPolicy": {
                            "type": "string"
                        },
                        "tag": {
                            "form": true,
                            "type": "string"
                        }
                    }
                },
                "replicas": {
                    "type": "integer"
                },
                "requests": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string",
                            "title": "WN CPUs",
                            "form": true,
                            "render": "slider",
                            "sliderMin": 1,
                            "sliderMax": 10000,
                            "sliderUnit": "m"
                        },
                        "memory": {
                            "type": "string",
                            "title": "WN Memory",
                            "form": true,
                            "render": "slider",
                            "sliderMin": 1,
                            "sliderMax": 16000,
                            "sliderUnit": "Mi"
                        }
                    }
                },
                "slotType": {
                    "type": "null"
                }
            }
        }
    }
}